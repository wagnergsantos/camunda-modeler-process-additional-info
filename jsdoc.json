{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": [
            "jsdoc",
            "closure"
        ]
    },
    "source": {
        "include": [
            "client"
        ],
        "exclude": [
            "client/client-bundle.js"
        ],
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "(^|[\\\\/])_"
    },
    "plugins": [],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "template": "node_modules/docdash",    
    "opts": {
        "destination": "./docs/jsdoc/",
        "encoding": "utf8",
        "private": true,
        "recurse": true,
        "template": "node_modules/docdash"
    },
    "docdash": {
        "static": true,
        "sort": true,
        "search": true,
        "collapse": true,
        "typedefs": true,
        "removeQuotes": "none",
        "scripts": []
    }
}